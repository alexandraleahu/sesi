<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY schema "http://schema.org/" >
    <!ENTITY dcterms "http://purl.org/dc/terms/" >
    <!ENTITY foaf "http://xmlns.com/foaf/0.1/" >
    <!ENTITY freebase "http://rdf.freebase.com/ns/" >
    <!ENTITY doap "http://usefulinc.com/ns/doap#" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY sesi "http://www.infoiasi.ro/wad/schemas/sesi/" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://www.infoiasi.ro/wad/schemas/sesi#"
     xml:base="http://www.infoiasi.ro/wad/schemas/sesi"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:sesi="http://www.infoiasi.ro/wad/schemas/sesi/"
     xmlns:dcterms="http://purl.org/dc/terms/"
     xmlns:schema="http://schema.org/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:freebase="http://rdf.freebase.com/ns/"
     xmlns:doap="http://usefulinc.com/ns/doap#">
    <owl:Ontology rdf:about="http://www.infoiasi.ro/wad/schemas/sesi"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://schema.org/member -->

    <owl:ObjectProperty rdf:about="&schema;member"/>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/belongsToCompany -->

    <owl:ObjectProperty rdf:about="&sesi;belongsToCompany">
        <rdf:type rdf:resource="&owl;AsymmetricProperty"/>
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdf:type rdf:resource="&owl;IrreflexiveProperty"/>
        <rdfs:range rdf:resource="&sesi;Company"/>
        <rdfs:domain rdf:resource="&sesi;Internship"/>
        <owl:inverseOf rdf:resource="&sesi;hasInternship"/>
    </owl:ObjectProperty>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/category -->

    <owl:ObjectProperty rdf:about="&sesi;category"/>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/hasInternship -->

    <owl:ObjectProperty rdf:about="&sesi;hasInternship">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="&sesi;Company"/>
        <rdfs:range rdf:resource="&sesi;Internship"/>
    </owl:ObjectProperty>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/hasStudent -->

    <owl:ObjectProperty rdf:about="&sesi;hasStudent">
        <rdfs:domain rdf:resource="&schema;CollegeOrUniversity"/>
        <rdfs:range rdf:resource="&sesi;Student"/>
    </owl:ObjectProperty>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/inCity -->

    <owl:ObjectProperty rdf:about="&sesi;inCity">
        <rdfs:range rdf:resource="&schema;City"/>
    </owl:ObjectProperty>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/isStudentOf -->

    <owl:ObjectProperty rdf:about="&sesi;isStudentOf">
        <rdfs:range rdf:resource="&schema;CollegeOrUniversity"/>
        <rdfs:subPropertyOf rdf:resource="&schema;member"/>
        <rdfs:domain rdf:resource="&sesi;Student"/>
        <owl:inverseOf rdf:resource="&sesi;hasStudent"/>
    </owl:ObjectProperty>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/technology -->

    <owl:ObjectProperty rdf:about="&sesi;technology">
        <rdf:type rdf:resource="&owl;AsymmetricProperty"/>
        <rdf:type rdf:resource="&owl;IrreflexiveProperty"/>
        <rdfs:range rdf:resource="&freebase;computer.software"/>
    </owl:ObjectProperty>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/workedOnProject -->

    <owl:ObjectProperty rdf:about="&sesi;workedOnProject">
        <rdf:type rdf:resource="&owl;AsymmetricProperty"/>
        <rdf:type rdf:resource="&owl;IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="&sesi;Student"/>
        <rdfs:range rdf:resource="&sesi;StudentProject"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/description -->

    <owl:DatatypeProperty rdf:about="&dc;description">
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://rdf.freebase.com/ns/time.event.end_date -->

    <owl:DatatypeProperty rdf:about="&freebase;time.event.end_date">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>endDate</rdfs:label>
        <rdfs:domain rdf:resource="&freebase;time.event"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://rdf.freebase.com/ns/time.event.start_date -->

    <owl:DatatypeProperty rdf:about="&freebase;time.event.start_date">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>startDate</rdfs:label>
        <rdfs:domain rdf:resource="&freebase;time.event"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/acquiredSkill -->

    <owl:DatatypeProperty rdf:about="&sesi;acquiredSkill">
        <rdfs:domain rdf:resource="&sesi;Internship"/>
        <rdfs:subPropertyOf rdf:resource="&sesi;skill"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/applicantsCount -->

    <owl:DatatypeProperty rdf:about="&sesi;applicantsCount">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&sesi;Internship"/>
        <rdfs:range rdf:resource="&xsd;nonNegativeInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/id -->

    <owl:DatatypeProperty rdf:about="&sesi;id">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/minimumRequirement -->

    <owl:DatatypeProperty rdf:about="&sesi;minimumRequirement">
        <rdfs:domain rdf:resource="&sesi;Internship"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/name -->

    <owl:DatatypeProperty rdf:about="&sesi;name">
        <rdfs:label>name</rdfs:label>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/offersRelocation -->

    <owl:DatatypeProperty rdf:about="&sesi;offersRelocation">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&sesi;Internship"/>
        <rdfs:range rdf:resource="&xsd;boolean"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/openingsCount -->

    <owl:DatatypeProperty rdf:about="&sesi;openingsCount">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&sesi;Internship"/>
        <rdfs:range rdf:resource="&xsd;nonNegativeInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/preferedSkill -->

    <owl:DatatypeProperty rdf:about="&sesi;preferedSkill">
        <rdfs:domain rdf:resource="&sesi;Internship"/>
        <rdfs:subPropertyOf rdf:resource="&sesi;skill"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/salary -->

    <owl:DatatypeProperty rdf:about="&sesi;salary">
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/skill -->

    <owl:DatatypeProperty rdf:about="&sesi;skill">
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://rdf.freebase.com/ns/computer.software -->

    <owl:Class rdf:about="&freebase;computer.software">
        <rdfs:label>Technology</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://www.freebase.com/computer/software"/>
    </owl:Class>
    


    <!-- http://rdf.freebase.com/ns/time.event -->

    <owl:Class rdf:about="&freebase;time.event">
        <rdfs:label>Event</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://www.freebase.com/time/event"/>
    </owl:Class>
    


    <!-- http://schema.org/City -->

    <owl:Class rdf:about="&schema;City">
        <rdfs:seeAlso rdf:resource="&schema;City"/>
    </owl:Class>
    


    <!-- http://schema.org/CollegeOrUniversity -->

    <owl:Class rdf:about="&schema;CollegeOrUniversity">
        <rdfs:label>School</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.infoiasi.ro/wad/schemas/sesi#Resource"/>
        <rdfs:seeAlso rdf:resource="&schema;CollegeOrUniversity"/>
    </owl:Class>
    


    <!-- http://schema.org/Corporation -->

    <owl:Class rdf:about="&schema;Corporation">
        <rdfs:seeAlso rdf:resource="&schema;Corporation"/>
    </owl:Class>
    


    <!-- http://usefulinc.com/ns/doap#Project -->

    <owl:Class rdf:about="&doap;Project">
        <rdfs:label>SoftwareProject</rdfs:label>
    </owl:Class>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi#Resource -->

    <owl:Class rdf:about="http://www.infoiasi.ro/wad/schemas/sesi#Resource">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&sesi;id"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="&xsd;integer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A user is someone who has an account and interacts with the application.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/Company -->

    <owl:Class rdf:about="&sesi;Company">
        <rdfs:subClassOf rdf:resource="&schema;Corporation"/>
        <rdfs:subClassOf rdf:resource="http://www.infoiasi.ro/wad/schemas/sesi#Resource"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&sesi;hasInternship"/>
                <owl:allValuesFrom rdf:resource="&sesi;Internship"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/Internship -->

    <owl:Class rdf:about="&sesi;Internship">
        <rdfs:subClassOf rdf:resource="&freebase;time.event"/>
        <rdfs:subClassOf rdf:resource="http://www.infoiasi.ro/wad/schemas/sesi#Resource"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&sesi;preferedSkill"/>
                <owl:someValuesFrom rdf:resource="&xsd;string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&sesi;acquiredSkill"/>
                <owl:someValuesFrom rdf:resource="&xsd;string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&sesi;applicantsCount"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="&xsd;nonNegativeInteger"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&sesi;openingsCount"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="&xsd;anyURI"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&freebase;time.event.start_date"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="&xsd;dateTime"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&sesi;category"/>
                <owl:someValuesFrom rdf:resource="&sesi;SoftwareCategory"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&sesi;offersRelocation"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="&xsd;boolean"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&sesi;salary"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="&xsd;decimal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&freebase;time.event.end_date"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="&xsd;dateTime"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&sesi;belongsToCompany"/>
                <owl:onClass rdf:resource="&sesi;Company"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/InternshipApplication -->

    <owl:Class rdf:about="&sesi;InternshipApplication"/>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/InternshipApplicationStatus -->

    <owl:Class rdf:about="&sesi;InternshipApplicationStatus">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&sesi;accepted"/>
                    <rdf:Description rdf:about="&sesi;pending"/>
                    <rdf:Description rdf:about="&sesi;rejected"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment xml:lang="en">The status of an application to the internship. Can only be of types: accepted, rejected</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/InternshipProgressStatus -->

    <owl:Class rdf:about="&sesi;InternshipProgressStatus">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&sesi;finished"/>
                    <rdf:Description rdf:about="&sesi;inProgress"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/SoftwareCategory -->

    <owl:Class rdf:about="&sesi;SoftwareCategory">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&sesi;cloudComputing"/>
                    <rdf:Description rdf:about="&sesi;databases"/>
                    <rdf:Description rdf:about="&sesi;qualityAssurance"/>
                    <rdf:Description rdf:about="&sesi;webDev"/>
                    <rdf:Description rdf:about="&sesi;businessSoftware"/>
                    <rdf:Description rdf:about="&sesi;gaming"/>
                    <rdf:Description rdf:about="&sesi;embedded"/>
                    <rdf:Description rdf:about="&sesi;mobile"/>
                    <rdf:Description rdf:about="&sesi;bigData"/>
                    <rdf:Description rdf:about="&sesi;management"/>
                    <rdf:Description rdf:about="&sesi;artificialIntelligence"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/Student -->

    <owl:Class rdf:about="&sesi;Student">
        <rdfs:subClassOf rdf:resource="http://www.infoiasi.ro/wad/schemas/sesi#Resource"/>
        <rdfs:subClassOf rdf:resource="&foaf;Person"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&sesi;name"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="&xsd;string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&sesi;isStudentOf"/>
                <owl:someValuesFrom rdf:resource="&schema;CollegeOrUniversity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/StudentProject -->

    <owl:Class rdf:about="&sesi;StudentProject">
        <rdfs:subClassOf rdf:resource="&doap;Project"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&sesi;technology"/>
                <owl:someValuesFrom rdf:resource="&freebase;computer.software"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A software project that a student has worked on</rdfs:comment>
    </owl:Class>
    


    <!-- http://xmlns.com/foaf/0.1/Person -->

    <owl:Class rdf:about="&foaf;Person">
        <rdfs:seeAlso rdf:resource="http://xmlns.com/foaf/spec/#term_Person"/>
    </owl:Class>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/accepted -->

    <owl:NamedIndividual rdf:about="&sesi;accepted">
        <rdf:type rdf:resource="&sesi;InternshipApplicationStatus"/>
        <rdfs:comment xml:lang="en">This application was accepted by the owning company</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/artificialIntelligence -->

    <owl:NamedIndividual rdf:about="&sesi;artificialIntelligence">
        <rdf:type rdf:resource="&sesi;SoftwareCategory"/>
        <rdfs:label xml:lang="en">Artificial Intelligence</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/bigData -->

    <owl:NamedIndividual rdf:about="&sesi;bigData">
        <rdf:type rdf:resource="&sesi;SoftwareCategory"/>
        <rdfs:label xml:lang="en">Big Data</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/businessSoftware -->

    <owl:NamedIndividual rdf:about="&sesi;businessSoftware">
        <rdf:type rdf:resource="&sesi;SoftwareCategory"/>
        <rdfs:label xml:lang="en">Business Software</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/cloudComputing -->

    <owl:NamedIndividual rdf:about="&sesi;cloudComputing">
        <rdf:type rdf:resource="&sesi;SoftwareCategory"/>
        <rdfs:label xml:lang="en">Cloud Computing and Networking</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/databases -->

    <owl:NamedIndividual rdf:about="&sesi;databases">
        <rdf:type rdf:resource="&sesi;SoftwareCategory"/>
        <rdfs:label xml:lang="en">Database Administration</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/embedded -->

    <owl:NamedIndividual rdf:about="&sesi;embedded">
        <rdf:type rdf:resource="&sesi;SoftwareCategory"/>
        <rdfs:label xml:lang="en">Embedded Software</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/finished -->

    <owl:NamedIndividual rdf:about="&sesi;finished">
        <rdf:type rdf:resource="&sesi;InternshipProgressStatus"/>
        <rdfs:comment xml:lang="en">The internship is finish (past its end date)</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/gaming -->

    <owl:NamedIndividual rdf:about="&sesi;gaming">
        <rdf:type rdf:resource="&sesi;SoftwareCategory"/>
        <rdfs:label xml:lang="en">Gaming and Graphics</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/inProgress -->

    <owl:NamedIndividual rdf:about="&sesi;inProgress">
        <rdf:type rdf:resource="&sesi;InternshipProgressStatus"/>
        <rdfs:comment xml:lang="en">The internship is in progress (between its start date and end date)</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/management -->

    <owl:NamedIndividual rdf:about="&sesi;management">
        <rdf:type rdf:resource="&sesi;SoftwareCategory"/>
        <rdfs:label xml:lang="en">IT Project Management and Consulting</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/mobile -->

    <owl:NamedIndividual rdf:about="&sesi;mobile">
        <rdf:type rdf:resource="&sesi;SoftwareCategory"/>
        <rdfs:label xml:lang="en">Mobile Development</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/pending -->

    <owl:NamedIndividual rdf:about="&sesi;pending">
        <rdf:type rdf:resource="&sesi;InternshipApplicationStatus"/>
        <rdfs:comment xml:lang="en">This application has not yet received an answer from the owning company</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/qualityAssurance -->

    <owl:NamedIndividual rdf:about="&sesi;qualityAssurance">
        <rdf:type rdf:resource="&sesi;SoftwareCategory"/>
        <rdfs:label xml:lang="en">Quality Assurance</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/rejected -->

    <owl:NamedIndividual rdf:about="&sesi;rejected">
        <rdf:type rdf:resource="&sesi;InternshipApplicationStatus"/>
        <rdfs:comment xml:lang="en">This application has been rejected by the owning company</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- http://www.infoiasi.ro/wad/schemas/sesi/webDev -->

    <owl:NamedIndividual rdf:about="&sesi;webDev">
        <rdf:type rdf:resource="&sesi;SoftwareCategory"/>
        <rdfs:label xml:lang="en">Web Development</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // General axioms
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <rdf:Description>
