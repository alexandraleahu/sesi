<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b='urn:import:com.github.gwtbootstrap.client.ui'>

    <g:HTMLPanel>
        <b:SimplePager display="{resTable}" location="CENTER"
                       fastForwardRows="5" />
        <b:CellTable ui:field="resTable" width="20" bordered="false" striped="false" condensed="true" pageSize="5" pageStart="0"/>

    </g:HTMLPanel>

</ui:UiBinder>