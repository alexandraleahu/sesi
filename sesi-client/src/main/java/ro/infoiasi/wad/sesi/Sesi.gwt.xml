<?xml version="1.0" encoding="UTF-8"?>
<!--
  When updating your version of GWT, you should also update this DTD reference,
  so that your app can take advantage of the latest GWT module capabilities.
-->
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.5.1//EN"
  "http://google-web-toolkit.googlecode.com/svn/tags/2.5.1/distro-source/core/src/gwt-module.dtd">
<module rename-to='sesi'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
    <inherits name="com.google.gwt.user.User" />
    <inherits name="com.google.gwt.resources.Resources" />
    <inherits name="com.google.gwt.editor.Editor" />


    <!-- Inherit the default GWT style sheet.  You can change       -->
    <!-- the theme of your GWT application by uncommenting          -->
    <!-- any one of the following lines.                            -->
    <inherits name='com.google.gwt.user.theme.clean.Clean'/>
    <!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->
    <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
    <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

    <!-- Other module inherits                                      -->
    <inherits name="com.github.gwtbootstrap.Bootstrap" />
    <inherits name="ro.infoiasi.wad.sesi.Core"/>
    <inherits name='com.github.gwtbootstrap.datepicker.Datepicker' />
    <inherits name='com.github.gwtbootstrap.datetimepicker.Datetimepicker' />
    <inherits name="com.google.common.base.Base"/>
    <inherits name="com.google.common.collect.Collect"/>

    <!-- Settings                                                   -->
    <set-property name="bootstrap.responsiveDesign" value="true"/>

    <!-- Specify the app entry point class.                         -->
    <entry-point class='ro.infoiasi.wad.sesi.client.Sesi'/>
<!--   <entry-point class='ro.infoiasi.wad.sesi.client.Sesi_client_gwt'/> -->
    
    
    <servlet path="/InternshipsService" class="ro.infoiasi.wad.sesi.server.rpc.InternshipsServiceImpl"/>
    <servlet path="/CompaniesService" class="ro.infoiasi.wad.sesi.server.rpc.CompaniesServiceImpl"/>
    <servlet path="/OntologyExtraInfoService" class="ro.infoiasi.wad.sesi.server.rpc.OntologyExtraInfoServiceImpl"/>
    <servlet path="/TechnologyService" class="ro.infoiasi.wad.sesi.server.rpc.TechnologyServiceImpl"/>
    <servlet path="/TechnicalSkillsService" class="ro.infoiasi.wad.sesi.server.rpc.TechnicalSkillsServiceImpl"/>
    <servlet path="/StudentService" class="ro.infoiasi.wad.sesi.server.rpc.StudentServiceImpl"/>
    <servlet path="/InternshipApplicationService"
             class="ro.infoiasi.wad.sesi.server.rpc.InternshipApplicationServiceImpl"/>
    <servlet path="/ProgressDetailsService" class="ro.infoiasi.wad.sesi.server.rpc.ProgressDetailsServiceImpl"/>

    <!-- Specify the paths for translatable code                    -->
    <source path='client'/>
    <source path='shared'/>
    <source path="resources" />
    <source path="core"/>

</module>
