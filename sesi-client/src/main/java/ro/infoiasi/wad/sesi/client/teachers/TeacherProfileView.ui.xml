<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b='urn:import:com.github.gwtbootstrap.client.ui'
             xmlns:c='urn:import:ro.infoiasi.wad.sesi.client.reports'
             xmlns:o='urn:import:ro.infoiasi.wad.sesi.client.ontologyextrainfo'
             xmlns:t="urn:import:ro.infoiasi.wad.sesi.client.teachers">


    <ui:with field="resource" type="ro.infoiasi.wad.sesi.resources.SesiResources"/>
    <g:HTMLPanel>
        <g:HTMLPanel>
            <b:WellForm>
                <b:Row>
                    <b:Column size="2">
                        <b:Label type="INFO" text="Name"/>
                    </b:Column>
                </b:Row>
                <b:Row>
                    <b:Column size="2">
                        <g:Label ui:field="nameLabel"/>
                    </b:Column>
                </b:Row>
                <b:Row>
                    <b:Column size="2">
                        <b:Label type="INFO" text="Title"/>
                    </b:Column>
                </b:Row>
                <b:Row>
                    <b:Column size="2">
                        <g:Label ui:field="titleLabel"/>
                    </b:Column>
                </b:Row>
                <b:Row>
                    <b:Column size="2">
                        <b:Label type="INFO" text="University"/>
                    </b:Column>
                </b:Row>
                <b:Row>
                    <b:Column size="2">
                        <o:OntologyExtraInfoView ui:field="universityLink"/>
                    </b:Column>
                </b:Row>
                <b:Row>
                    <b:Column size="2">
                        <b:Label type="INFO" text="Faculty"/>
                    </b:Column>
                </b:Row>
                <b:Row>
                    <b:Column size="2">
                        <o:OntologyExtraInfoView ui:field="facultyLink"/>
                    </b:Column>

                </b:Row>
                <b:Row>
                    <b:Column size="2">
                        <b:Label type="INFO" text="Site"/>
                    </b:Column>
                </b:Row>
                <b:Row>
                    <b:Column size="2">
                        <g:Anchor ui:field="siteLabel"/>
                    </b:Column>
                </b:Row>
                <b:Row>
                    <b:Column size="2">
                        <b:Label type="INFO" text="Courses"/>
                    </b:Column>
                </b:Row>
                <b:Row>
                    <b:Column size="3">
                        <o:OntologyExtraInfoListView ui:field="coursesTable"/>
                    </b:Column>
                </b:Row>
            </b:WellForm>
        </g:HTMLPanel>
    </g:HTMLPanel>
</ui:UiBinder>