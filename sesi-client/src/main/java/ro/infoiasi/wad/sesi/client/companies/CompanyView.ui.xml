<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b='urn:import:com.github.gwtbootstrap.client.ui'
        >
    <ui:with field="resource" type="ro.infoiasi.wad.sesi.resources.SesiResources"/>
    <g:HTMLPanel styleName="{resource.style.withScroll}">
        <b:WellForm>
            <b:Row>
                <b:Column size="1">
                    <b:Label type="INFO" text="Name"/>
                </b:Column>
            </b:Row>
            <b:Row>
                <b:Column size="4">
                    <b:Label ui:field="name"/>
                </b:Column>
            </b:Row>
            <b:Row>
                <b:Column size="1">
                    <b:Label type="INFO" text="Description"/>
                </b:Column>
            </b:Row>
            <b:Row>
                <b:Column size="4">
                    <b:Label ui:field="description"/>
                </b:Column>
            </b:Row>
            <b:Row>
                <b:Column size="1">
                    <b:Label type="INFO" text="Active"/>
                </b:Column>
            </b:Row>
            <b:Row>
                <b:Column size="4">
                    <b:Label ui:field="active"/>
                </b:Column>
            </b:Row>
            <b:Row>
                <b:Column size="1">
                    <b:Label type="INFO" text="Company url"/>
                </b:Column>
            </b:Row>
            <b:Row>
                <b:Column size="4">
                    <b:Label ui:field="companyUrl"/>
                </b:Column>
            </b:Row>
            <b:Row>
                <b:Column size="1">
                    <b:Label type="INFO" text="Community rating"/>
                </b:Column>
            </b:Row>
            <b:Row>
                <b:Column size="4">
                    <b:Label ui:field="communityRating"/>
                </b:Column>
            </b:Row>
        </b:WellForm>
    </g:HTMLPanel>
</ui:UiBinder>