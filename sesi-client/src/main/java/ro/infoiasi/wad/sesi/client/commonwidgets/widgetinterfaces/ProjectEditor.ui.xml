<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b='urn:import:com.github.gwtbootstrap.client.ui'
        >
    <ui:with field="resource" type="ro.infoiasi.wad.sesi.resources.SesiResources" />
    <g:HTMLPanel>
        <b:WellForm>
            <b:Row>
                <b:Column size="1">
                    <b:Label type="INFO" text="Name"/>
                </b:Column>
            </b:Row>
            <b:Row>
                <b:Column size="2">
                    <b:TextBox ui:field="projectName" size="2" placeholder="Name"/>
                </b:Column>
            </b:Row>
            <b:Row>
                <b:Column size="1">
                    <b:Label type="INFO" text="Description"/>
                </b:Column>
            </b:Row>
            <b:Row>
                <b:Column size="2">
                    <b:TextBox ui:field="projectDescription" size="2" placeholder="Description"/>
                </b:Column>
            </b:Row>
            <b:Row>
                <b:Column size="1">
                    <b:Label type="INFO" text="Project url"/>
                </b:Column>
            </b:Row>
            <b:Row>
                <b:Column size="2">
                    <b:TextBox ui:field="projectUrl" size="2" placeholder="Project url"/>
                </b:Column>
            </b:Row>
            <b:Row>
                <b:Column size="1">
                    <b:Label type="INFO" text="Repository"/>
                </b:Column>
            </b:Row>
            <b:Row>
                <b:Column size="2">
                    <b:TextBox ui:field="repository" size="2" placeholder="Repository"/>
                </b:Column>
            </b:Row>
            <b:Row>
                <b:Column size="2">
                    <b:Label type="INFO" text="Used programming languages"/>
                </b:Column>
            </b:Row>
            <b:Row>
                <b:Column size="3">
                    <b:TextBox ui:field="programmingLanguage" b:id="freebaseProgrammingLanguages" size="3"
                               placeholder="Freebase search programming languages"/>
                </b:Column>
            </b:Row>
            <b:Row>
                <b:Column size="5">
                    <b:TextArea ui:field="programmingLanguagesIdArea" b:id="programmingLanguagesId"
                                styleName="{resource.style.standardTextArea}" size="5"
                                placeholder="Selected programming languages will go here"/>
                </b:Column>
            </b:Row>
            <b:Row>
                <b:Column size="2">
                    <b:Label type="INFO" text="Used technologies"/>
                </b:Column>
            </b:Row>
            <b:Row>
                <b:Column size="3">
                    <b:TextBox ui:field="technologies" b:id="freebaseTeechnologies" size="3"
                               placeholder="Freebase search technologies"/>
                </b:Column>
            </b:Row>
            <b:Row>
                <b:Column size="5">
                    <b:TextArea ui:field="technologiesIdArea" b:id="technologiesId"
                                styleName="{resource.style.standardTextArea}" size="5"
                                placeholder="Selected technologies will go here"/>
                </b:Column>
            </b:Row>
            <b:Row>
                <b:Column size="2">
                    <b:Button ui:field="deleteProject" text="Remove"/>
                </b:Column>
            </b:Row>


        </b:WellForm>

    </g:HTMLPanel>
</ui:UiBinder>