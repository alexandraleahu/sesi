<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
    <servlet>
        <servlet-name>InternshipsServlet</servlet-name>
        <servlet-class>ro.infoiasi.wad.sesi.server.rpc.InternshipsServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ro.infoiasi.wad.sesi.Sesi CompaniesService</servlet-name>
        <servlet-class>ro.infoiasi.wad.sesi.server.rpc.CompaniesServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ro.infoiasi.wad.sesi.Sesi OntologyExtraInfoService</servlet-name>
        <servlet-class>ro.infoiasi.wad.sesi.server.rpc.OntologyExtraInfoServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ro.infoiasi.wad.sesi.Sesi TechnologyService</servlet-name>
        <servlet-class>ro.infoiasi.wad.sesi.server.rpc.TechnologyServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ro.infoiasi.wad.sesi.Sesi TechnicalSkillsService</servlet-name>
        <servlet-class>ro.infoiasi.wad.sesi.server.rpc.TechnicalSkillsServiceImpl</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>InternshipsServlet</servlet-name>
        <url-pattern>/sesi/InternshipsService</url-pattern>

    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ro.infoiasi.wad.sesi.Sesi CompaniesService</servlet-name>
        <url-pattern>/sesi/CompaniesService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ro.infoiasi.wad.sesi.Sesi OntologyExtraInfoService</servlet-name>
        <url-pattern>/sesi/OntologyExtraInfoService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ro.infoiasi.wad.sesi.Sesi TechnologyService</servlet-name>
        <url-pattern>/sesi/TechnologyService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ro.infoiasi.wad.sesi.Sesi TechnicalSkillsService</servlet-name>
        <url-pattern>/sesi/TechnicalSkillsService</url-pattern>
    </servlet-mapping>


    <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>Sesi.html</welcome-file>
  </welcome-file-list>

</web-app>
